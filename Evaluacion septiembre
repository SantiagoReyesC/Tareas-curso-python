    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = np.linspace(-0.5, 2.5, 100)\n",
    "y = x**2\n",
    "\n",
    "# linea tangente a (1,1): y = 2x - 1\n",
    "x_tangent = np.linspace(-0.5, 2.5, 50)\n",
    "y_tangent = 2 * x_tangent - 1\n",
    "\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.plot(x, y, 'b-', linewidth=2, label='y = x²')\n",
    "plt.plot(x_tangent, y_tangent, 'g-', linewidth=2, label='Tangente: y = 2x - 1')\n",
    "\n",
    "\n",
    "\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('y')\n",
    "plt.title('y = x² y linea tangente al (1,1)')\n",
    "plt.grid(True, alpha=0.3)\n",
    "plt.legend()\n",
    "plt.axhline(y=0, color='k', linestyle='-')\n",
    "plt.axvline(x=0, color='k', linestyle='-')\n",
    "\n",
    "plt.show()"
   ]
