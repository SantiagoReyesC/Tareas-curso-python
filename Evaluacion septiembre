    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Create a completely black 256x256 image (all zeros)\n",
    "black_image = np.zeros((256, 256), dtype=np.uint8)\n",
    "\n",
    "# Display the image\n",
    "plt.figure(figsize=(8, 8))\n",
    "plt.imshow(black_image, cmap='gray', vmin=0, vmax=255)\n",
    "plt.title('256×256 Completely Black Image')\n",
    "\n",
    "plt.show()\n",
    "\n"
   ]
  },
